{
  "name": "vscode-copy-from-terminal",
  "displayName": "Copy/Pipe From Terminal",
  "description": "Copy/pipe from terminal to VS Code editor",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/babakks/vscode-copy-from-terminal"
  },
  "engines": {
    "vscode": "^1.60.0"
  },
  "os": [
    "linux",
    "darwin"
  ],
  "categories": [
    "Other"
  ],
  "keywords": [
    "terminal",
    "copy",
    "pipe",
    "stdout",
    "stdin",
    "editor",
    "shell",
    "bash",
    "zsh",
    "productivity",
    "linux",
    "mac",
    "macos",
    "unix",
    "watch"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "icon": "images/icon.png",
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-copy-from-terminal.toggle",
        "title": "Copy From Terminal: Toggle (Enable/Disable)",
        "shortTitle": "Toggle"
      }
    ],
    "configuration": [
      {
        "title": "Copy/Pipe From Terminal",
        "properties": {
          "vscode-copy-from-terminal.enabled": {
            "type": "boolean",
            "default": true
          },
          "vscode-copy-from-terminal.tempDirectory": {
            "type": "string",
            "default": ""
          },
          "vscode-copy-from-terminal.alias": {
            "type": "string",
            "default": "cp2code"
          }
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.60.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.11.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.2",
    "typescript": "^4.5.5"
  }
}
